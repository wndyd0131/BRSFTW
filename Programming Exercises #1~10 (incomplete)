#include <stdio.h>
void function_1() {
	int num, i;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 1; i <= 10; i++) {
		printf("%d * %d = %d\n", num, i, num * i);
	}
}

void function_2(){
	int num, i, j;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 0; i < num; i++) {
		for (j = 0; j <= i; j++)
			printf("*");
		printf("\n");
	}
}

void function_3() {
	int num, i, j;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 0; i < num; i++) {
		for (j = 0; j <= i; j++)
			printf("%d", j + 1);
		printf("\n");

	}
}

void function_4() {
	int num, i, j;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 0; i < num; i++) {
		for (j = 0; j <= i; j++)
			printf("%d", i+1);
		printf("\n");
	}
}

void function_5() {
	int num, i, j, k = 1;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 0; i < num; i++) {
		for (j = 0; j <= i; j++)
			printf("%d ", k++);
		printf("\n");
	}
}


void function_6() {
	int i, j, k, num;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 1; i <= num; i++) {
		for (k = 0; k < num - i; k++) {
			printf("%c", ' ');
		}
		for (j = 0; j < i; j++) {
			printf("%s", " *");
		}
		printf("%c", '\n');
	}
	return;
}

void function_7() {
	int i, j, k, l = 1, num;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 1; i <= num; i++) {
		for (k = 0; k < num - i; k++) {
			printf("%c", ' ');
		}
		for (j = 0; j < i; j++) {
			printf(" %d", l++);
		}
		printf("\n");
	}
}
void function_8() {
	int i, j, k, num;
	printf("Input the number: ");
	scanf_s("%d", &num);
	for (i = 1; i <= num; i++) {
		for (k = 0; k < num - i; k++) {
			printf("%c", ' ');
		}
		for (j = 0; j < i; j++) {
			printf(" %d", i);
		}
		printf("\n");
	}
}

void function_9() {
	int i, j, k = 0, num;
	printf("Input the number: ");
	scanf("%d", &num);
	for (i = 1; i <= num; i++) {
		for (j = 0; j < i; j++) {
			if (k == 0) {
				k = 1;
				printf("%d", k);
			}
			else {
				k = 0;
				printf("%d", k);
			}
		}
		printf("\n");
		if (i % 2 == 0)
			k = 0;
		else {
			k = 1;
		}
	}
}

