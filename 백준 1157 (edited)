#include <stdio.h>
#include <string.h>
#include <stdbool.h>
Get_String(char *str) {
	printf("문자열을 입력하시오: ");
	scanf("%s", str);
	return;
}

Comparing(char *str) {
	char AlphaCnt[26] = { 0 };
	bool TorF = true;
	int len = strlen(str), i, max, temp;
	for (i = 0; i < len; i++) {
		if (str[i] >= 'a' && str[i] <= 'z')
		str[i] -= 32;
	}
	for (i = 0; i < len; i++) {
		if (str[i] >= 'A' && str[i] <= 'Z') {
			AlphaCnt[str[i] - 'A']++;
		}
	}
	max = 0;
	for (i = 0; i < 26; i++) {
		if (max < AlphaCnt[i]) {
			max = AlphaCnt[i];
			temp = i;
		}
	}
	for (i = 0; i < 26; i++) {
		if (temp == i) { //최대빈도의 인덱스와 일반빈도의 인덱스 비교
			continue;
		}
		if (max == AlphaCnt[i]) //최대빈도와 일반빈도 비교 
			TorF = false;
	}
	if (TorF == false)
		printf("?\n");
	else
		printf("%c가 제일 많음\n", 'A' + temp);
}

int main() {
	char str[50];
	Get_String(str);
	Comparing(str);
}
